{
  "name": "platzi-user-api",
  "version": "1.1.0",
  "description": "Unofficial API RESTful for Platzi user",
  "main": "build/index.js",
  "scripts": {
    "start:build": "npm run build && npm start",
    "start": "node .",
    "test": "mocha -r ts-node/register src/**/test.ts --timeout 15000 --slow 7000 --exit",
    "test:prod": "mocha build/**/test.js --timeout 15000 --slow 7000 --exit",
    "dev": "nodemon",
    "build": "tsc",
    "tslint": "tslint --project ./",
    "tslint:fix": "npm run tslint -- --fix",
    "check": "bash ./check.sh"
  },
  "keywords": [
    "platzi",
    "user",
    "api"
  ],
  "author": "Jecsham Castillo",
  "license": "MIT",
  "dependencies": {
    "apicache": "^1.2.3",
    "consola": "^2.6.0",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-rate-limit": "^3.3.2",
    "mongoose": "^5.3.14",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@types/apicache": "^1.2.0",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/express-rate-limit": "^3.3.0",
    "@types/mocha": "^5.2.6",
    "@types/mongoose": "^5.5.1",
    "@types/node": "^12.0.2",
    "@types/request-promise": "^4.1.43",
    "@types/serve-favicon": "^2.2.30",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.7",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
